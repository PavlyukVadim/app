{
  "name": "app.github.io",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "preact": "^8.1.0"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "babel-brunch": "~6.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-latest": "^6",
    "babel-preset-stage-0": "^6.24.1",
    "brunch": "^2",
    "chai": "^4.1.1",
    "clean-css-brunch": "^2",
    "enzyme": "^2.9.1",
    "handlebars-brunch": "^2.10.0",
    "jest": "^20.0.4",
    "preact-jsx-chai": "^2.2.1",
    "react-addons-test-utils": "^15.6.0",
    "uglify-js-brunch": "^2"
  },
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --production",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PavlyukVadim/app.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ]
  },
  "bugs": {
    "url": "https://github.com/PavlyukVadim/app.github.io/issues"
  },
  "homepage": "https://github.com/PavlyukVadim/app.github.io#readme"
}
